<header id="user-index-header">
  <div class="git-user-info">
    {{#link-to 'user' model.gitUser.login}}
      <img class="avatar-img" src="{{model.gitUser.avatarUrl}}" alt="">
      <h3>{{model.gitUser.login}}</h3>
    {{/link-to}}
  </div>
  <h2>Library of Devexandria</h2>
  {{#if session.isAuthenticated}}
    <button {{action 'logout'}}>logout</button>
  {{/if}}
</header>
<div class="sub-header-library">
  <button class="subscribe-btn" {{action 'subscribeUser'}}>Subscribe to this library</button>
  <h2>{{model.library.user.username}}'s Library</h2>
</div>
{{outlet}}
